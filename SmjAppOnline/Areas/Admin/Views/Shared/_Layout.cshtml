                                <img src="~/images/logo.png" width="100" />
                            </a>
                        </div>
                        <!-- Grid column -->
                        <div class="col-md-2">
                            <h6 class="text-uppercase font-weight-bold">
                                <a asp-area="" asp-controller="Home" asp-action="AboutUs">About Us</a>
                            </h6>
                        </div>
                        <!-- Grid column -->
                        <!-- Grid column -->
                        <div class="col-md-2">
